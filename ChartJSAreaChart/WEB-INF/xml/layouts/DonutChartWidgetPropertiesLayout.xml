 <!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
 <mstrlayout:layout>
    <div>
 
    <span style="font-weight:bold;">Filled Color &nbsp; <span style="font-weight: normal;">(e.g. #FFFFFF)</span>:</span>
    <br />
    <mstrlayout:render name="Textbox">
       <mstrlayout:argument type="string" value="filledColor" />
    </mstrlayout:render>
    <br />
    <br />   
    
    <span style="font-weight:bold;">UnFilled Color &nbsp; <span style="font-weight: normal;">(e.g. #FFFFFF)</span>:</span>
    <br />
    <mstrlayout:render name="Textbox">
       <mstrlayout:argument type="string" value="unFilledColor" />
    </mstrlayout:render>
    <br />
    <br /> 
    
    <span style="font-weight:bold;">Value Color &nbsp; <span style="font-weight: normal;">(e.g. #FFFFFF)</span>:</span>
    <br />
    <mstrlayout:render name="Textbox">
       <mstrlayout:argument type="string" value="valueColor" />
    </mstrlayout:render>
    <br />
    <br /> 
    
    <span style="font-weight:bold;">Font Family &nbsp; <span style="font-weight: normal;">(e.g. Arial)</span>:</span>
    <br />
    <mstrlayout:render name="Textbox">
       <mstrlayout:argument type="string" value="fontFamily" />
    </mstrlayout:render>
    <br />
    <br />
    
     
    </div>
 </mstrlayout:layout>